{
  "files.associations": {
    "*.h": "objective-c",
    "*.m": "objective-c",
    "*.mm": "objective-cpp"
  },
  "C_Cpp.default.intelliSenseMode": "macos-clang-arm64",
  "C_Cpp.default.compilerPath": "/usr/bin/clang",
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.default.cppStandard": "c++17",
  "editor.formatOnSave": false,
  "[objective-c]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "[objective-cpp]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  }
}
